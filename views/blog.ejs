<div class="page-header" data-parallax="true" id="erics_media" style="background-image: url('/original/home (4).jpg')">
    <div class="container">
        <div class="row">
            <div class="col-md-8 ml-auto mr-auto">
                <% if(error && error.length > 0){ %>
                <div class="alert alert-danger" role="alert">
                    <%= error %>
                </div>
                <% } %>
                <% if(success && success.length > 0){ %>
                <div class="alert alert-success" role="alert">
                    <%= success %>
                </div>
                <% } %>
                <div class="brand">
                    <h1 style="font-family: 'Ubuntu', cursive;" id="logo_color"
                        class="text_shadow align-content-center ">Welcome to the Blog page</h1>
                </div>
            </div>
        </div>
    </div>
</div>


